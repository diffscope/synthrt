{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        "qmsetup",
        "nlohmann-json",
        "stduuid",
        "stdcorelib",
        "blake3",
        "sparsepp",
        "bit7z"
    ],
    "features": {
        "tests": {
            "description": "Enable test cases support",
            "dependencies": [
                "boost-test"
            ]
        }
    },
    "vcpkg-configuration": {
        "overlay-ports": [
            "../vcpkg/ports"
        ],
        "overlay-triplets": [
            "../vcpkg/triplets"
        ]
    }
}